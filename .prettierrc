{
  "useTabs": false,
  "tabWidth": 2,
  "jsxBracketSameLine": false,
  "bracketSpacing": true,
  "printWidth": 110,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "printWidth": 90
      }
    },
    {
      "files": "*.md",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
