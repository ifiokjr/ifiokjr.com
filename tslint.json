{
  "defaultSeverity": "error",
  "extends": ["@kj/tslint-rules"],
  "rules": {
    "interface-name": false,
    "semicolon": [true, "never"],
    "member-access": [false],
    "ordered-imports": [true],
    "no-console": [false],
    "no-var-requires": true,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
