{
  "name": "ifiokjr.com",
  "description": "ifiokjr.com website",
  "version": "1.0.0",
  "author": "Ifiok Jr.",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "lint": "eslint -f friendly --ext=.tsx,.ts,.js .",
    "fix": "yarn lint --fix",
    "postinstall": "patch-package",
    "format": "prettier --write \"{,!(node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\"",
    "typecheck": "tsc -p ./tsconfig.json"
  },
  "keywords": [
    "gatsby",
    "typescript"
  ],
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "@theme-ui/prism": "^0.2.50",
