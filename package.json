    "deepmerge": "4.2.2",
    "gatsby": "^2.18.17",
    "gatsby-image": "^2.2.37",
    "gatsby-link": "^2.2.27",
    "gatsby-plugin-catch-links": "^2.1.21",
    "gatsby-plugin-feed": "^2.3.25",
    "gatsby-plugin-flow": "^1.1.17",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-google-gtag": "^1.1.19",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-mdx": "^1.0.64",
    "gatsby-plugin-netlify": "^2.1.30",
    "gatsby-plugin-netlify-cms": "^4.1.33",
    "gatsby-plugin-offline": "^3.0.30",
    "gatsby-plugin-optimize-svgs": "^1.0.3",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sharp": "^2.3.10",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-theme-ui": "^0.2.53",
    "gatsby-plugin-twitter": "^2.1.17",
    "gatsby-plugin-typegen": "^0.2.0",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-remark-autolink-headers": "^2.1.21",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.39",
    "gatsby-remark-katex": "^3.1.20",
    "gatsby-remark-prismjs": "^3.3.28",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.30",
    "gatsby-remark-smartypants": "^2.1.19",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "gatsby-transformer-sharp": "^2.3.9",
    "graphql": "^14.5.8",
    "graphql-compose": "^6.3.8",
    "graphql-tag": "^2.10.1",
    "graphql-tag-pluck": "^0.8.7",
    "invariant": "^2.2.4",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.10.1",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "source-map-support": "^0.5.16",
    "theme-ui": "^0.2.52",
    "ts-node": "^8.5.4",
    "typography-theme-twin-peaks": "^0.16.19"
  },
  "devDependencies": {
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/theme-ui": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "lint-staged": "^9.5.0",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.19.1",
    "prettier-package-json": "^2.1.3",
    "typescript": "^3.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.{json,yml,yaml,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "**/.babelrc.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "jsxBracketSameLine": false,
    "bracketSpacing": true,
    "printWidth": 110,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "preserve",
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "printWidth": 90
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifiokjr/ifiokjr.com"
  }
}
